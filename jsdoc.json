{
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  },
  "opts": {
    "template": "node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "title": "p5.beholder",
      "menu": [
        {
          "title": "⚒️ Made by Enric Llagostera",
          "link": "https://enric.llagostera.com.br/",
          "target": "_blank"
        },
        {
          "title": "📂 GitHub",
          "link": "https://github.com/enricllagostera/p5.beholder",
          "target": "_blank",
          "class": "github-link",
          "id": "repository"
        },

        {
          "title": "📦 NPM",
          "link": "https://www.npmjs.com/package/p5.beholder",
          "target": "_blank"
        }
      ]
    }
  },
  "plugins": ["plugins/markdown"],

  "docdash": {
    "static": true,
    "sort": true,
    "search": true,
    "collapse": true,
    "typedefs": true,
    "removeQuotes": "none",
    "scripts": [],
    "menu": {
      "Github repo": {
        "href": "https://github.com/enricllagostera/p5.beholder",
        "target": "_blank",
        "class": "menu-item",
        "id": "repository"
      }
    }
  },
  "templates": {
    "default": {
      "useLongnameInNav": true
    }
  }
}
